<Window x:Class="Pchecker.View.ProjektDetailsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pchecker"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:Pchecker.ViewModel" 
        Title="ProjektDetailsWindow" Height="536" Width="1290">
    <Window.DataContext>
        <vm:ViewModelProjektDetails></vm:ViewModelProjektDetails>
    </Window.DataContext>
    <Grid>
        <DataGrid ItemsSource="{Binding Projekt}"/>
    </Grid>
</Window>
